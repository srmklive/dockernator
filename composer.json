{
    "name": "srmklive/dockernator",
    "type": "library",
    "description": "PHP port of name generator used by Docker Desktop to generate container names.",
    "keywords": ["generator","names", "names generator"],
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
      {
        "name": "Raza Mehdi",
        "email": "srmk@outlook.com"
      }
    ],
    "autoload":{
      "psr-4": {
        "Srmklive\\Dockernator\\": "src/"
      }
    },
    "require": {
        "php": ">=7.2|^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^8.0|^9.0|^10.0",
        "symfony/var-dumper": "~5.0"
    },
    "config": {
        "preferred-install": "dist"
    }
  }